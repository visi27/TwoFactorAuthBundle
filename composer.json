{
  "name": "evis/twofactor-bundle",
  "type": "symfony-bundle",
  "description": "This Bundle enables two-factor authentication through Google Authenticator or email",
  "keywords": ["security", "2fa", "auth"],
  "homepage": "https://github.com/visi27/TwoFactorAuthBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Evis Bregu",
      "email": "evis.bregu@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "symfony/symfony": ">=3.2",
    "sensio/framework-extra-bundle": "~3.0",
    "doctrine/orm": "^2.5",
    "doctrine/doctrine-bundle": "^1.6",
    "sonata-project/google-authenticator": ">=1.1"
  },
  "require-dev": {
    "phpunit/phpunit": ">=5.4,<6.0",
    "symfony/phpunit-bridge": "~3.0"
  },
  "autoload": {
    "psr-4": { "Evis\\TwoFactorAuthBundle\\": "" },
    "exclude-from-classmap": [ "/Tests/" ]
  },
  "prefer-stable": true
}